struct test {
  A : int,
  B : real
}

reactor oldcounter(click: stream test) {
    sum: cell int = 0 .. sum + inc;
private:
    inc: stream int = 1 @ click;
}

reactor counter(inc : stream unit,
                dec : stream unit) {
    value:  cell int = 0 .. value + change;
private:
    change: stream int =  0 @ inc and dec
                       |  1 @ inc
                       | -1 @ dec;
}
